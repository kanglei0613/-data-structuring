        "import random\n",
        "\n",
        "def lotto(n, m):\n",
        "    numbers = list(range(1, n + 1))\n",
        "    result = []\n",
        "\n",
        "    for _ in range(m):\n",
        "        pick = random.choice(numbers)\n",
        "        numbers.remove(pick)\n",
        "        result.append(pick)\n",
        "\n",
        "    return result\n"
      ]
