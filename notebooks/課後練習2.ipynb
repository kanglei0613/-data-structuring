      "source": [
        "num_cans = int(input(\"請輸入購買飲料的罐數？\"))\n",
        "\n",
        "single_can_price = 20\n",
        "dozen_price = 200\n",
        "dozen_size = 12\n",
        "\n",
        "dozen_count = num_cans // dozen_size\n",
        "remaining_cans = num_cans % dozen_size\n",
        "\n",
        "total_cost = dozen_count * dozen_price + remaining_cans * single_can_price\n",
        "\n",
        "print(f\"需花費 {total_cost}\")\n"
      ]
